<h1>Sign Up</h1>
<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_area :name %>
<%= f.label :password %>
<%= f.text_area :password %>
<%= f.number_field :nausea %>
<%= f.number_field :happiness %>
<%= f.number_field :tickets %>
<%= f.number_field :height %>
<%= f.check_box :admin %>
<%= f.submit %>
<%end%>